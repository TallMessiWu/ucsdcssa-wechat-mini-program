.container {
    padding-top: 30rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}

.cssa-card {
    background-image: url(data:image/jpeg;base64,);
    background-size: cover;
    height: 400rpx;
    width: 634rpx;
    position: relative;
    display: flex;
    justify-content: center;
    flex-direction: column;
    align-items: center;
    color: #fff;
    font-weight: 300;
    text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
    border-radius: 15px;
}

.logo {
    height: 180rpx;
    width: 634rpx;
    background-color: white;
    position: absolute;
    top: 45rpx;
    left: 0
}

.number {
    position: absolute;
    font-size: 56rpx;
    font-weight: 500;
    bottom: 60rpx;
}

.login-mask {
    position: absolute;
    height: 400rpx;
    width: 634rpx;
    background-color: rgba(128, 128, 128, 0.8);
    border-radius: 15px;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.form {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
    background-color: #FFFFFF;
    z-index: 10;
    border-radius: 15px;
    margin-bottom: 10rpx;
}

.button {
    margin-right: 5rpx;
}


.card-info {
    background-color: #fafafa;
    width: 634rpx;
    border-radius: 15px;
    margin-top: 30rpx;
    padding-right: 10rpx;
}

.l-title {
    margin-top: 5rpx;
    margin-left: 10rpx;
    color: #C22A1E;
}

.description {
    padding-bottom: 10rpx;
    padding-right: 30rpx;
}


.merchants-list {
    width: 550rpx;
    height: 800rpx;
}

.stand-view {
    width: 100%;
    height: 200rpx;
}